{
	"repositories": [
		{
			"packagist": false
		},
		{
			"type": "composer",
			"url": "http://packagist.cn"
		}
	],
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.38",
        "predis/predis": "^1.0",
	    "maatwebsite/excel": "~2.1.0",
        "guzzlehttp/guzzle": "^6.1",
        "zf-commons/zfc-rbac": "~2.4",
        "barryvdh/laravel-ide-helper": "^2.1",
        "dcn/rbac": "~1.1.0",
	    "ixudra/curl": "5.*",
		"bosnadev/repositories": "^0.12.0",
		"anlutro/l4-repository": "^0.7.9",
		"league/pipeline": "^0.2.2",
		"php-amqplib/php-amqplib": "^2.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
	    "symfony/dom-crawler":"~3.0",
	    "symfony/css-selector":"~3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Gather/Utils/loader.php",
            "vendor/aliyun/aliyun-openapi-php-sdk/aliyun-php-sdk-core/Config.php",
            "vendor/aliyun/sms/TopSdk.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
	 	"secure-http": false
    }
}
